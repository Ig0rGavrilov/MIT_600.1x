   ],
   "source": [
    "epsilon = 0.01\n",
    "y = 54\n",
    "guess  = y / 2.0\n",
    "numGuesses = 0\n",
    "\n",
    "while abs(guess*guess - y) >=epsilon:\n",
    "    numGuesses +=1\n",
    "    guess = guess - (((guess**2) - y)/(2*guess))\n",
    "\n",
    "print('numGuesses = ' + str(numGuesses))\n",
    "print('Square root of ' + str(y) + ' is about ' + str(guess))"
   ]
