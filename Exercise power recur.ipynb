   "source": [
    "def recurPower(base, exp):\n",
    "    '''\n",
    "    base: int or float.\n",
    "    exp: int >= 0\n",
    " \n",
    "    returns: int or float, base^exp\n",
    "    '''\n",
    "    result=1\n",
    "    if exp ==0:\n",
    "        return result\n",
    "    else:\n",
    "        return base*recurPower(base, exp-1)\n"
   ]
