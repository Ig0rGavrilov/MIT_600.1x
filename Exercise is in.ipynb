   "source": [
    "def isIn(char, aStr):\n",
    "    if not aStr:\n",
    "        return False\n",
    "    mid = len(aStr) // 2\n",
    "    if char == aStr[mid]:\n",
    "        return True\n",
    "    elif char > aStr[mid]:\n",
    "        return isIn(char, aStr[mid+1:])\n",
    "    return isIn(char, aStr[:mid])"
   ]
